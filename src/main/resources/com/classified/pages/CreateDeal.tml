<html t:type="layout" title="Create a new deal"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter"  >


        <t:beaneditform t:id="form" object="deal" submitlabel="create new deal" add="username,passwd,imageFile">
            <p:username>
                 <t:label for="username"/>
                 <t:textfield t:id="username"/>
                 </p:username>
            <p:passwd>
                <t:label for="passwd"/>
                <t:passwordfield t:id="passwd"/>
            </p:passwd>

            <p:imageFile>
                <t:label for="file">Choose an image </t:label>
                <input t:type="upload" t:id="file" t:value="file"/>
            </p:imageFile>


        </t:beaneditform>



</html>
